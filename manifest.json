{
    "manifest_version": 3,
    "name": "Group Leads",
    "short_name": "Group Leads",
    "version": "3.9.4",
    "description": "Group leads helps you convert new facebook group members into leads in your favorite email marketing software.",
    "background": {
        "service_worker": "background/bg-loader.js"
    },
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "128": "icon128.png"
    },
    "action": {
        "browser_style": false,
        "default_title": "Group Leads",
        "default_popup": "/login.html"
    },
    "declarative_net_request": {
        "rule_resources": [{
          "id": "ruleset_1",
          "enabled": true,
          "path": "rules.json"
        }]
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "oauth2": {
        "client_id": "1078285110766-o89r1u02fda5ope8uc810o12fo80b2bh.apps.googleusercontent.com",
        "scopes": ["https://www.googleapis.com/auth/spreadsheets"]
    },
    "permissions": ["tabs","storage", "cookies", "identity", "notifications","declarativeNetRequest","alarms"],
    "host_permissions": ["*://*.facebook.com/*", "*://*.groupleads.net/*", "*://*.api-us1.com/*","*://*.google.com/*","*://*.googleapis.com/*"],
    "content_scripts": [{
        "matches": ["*://*.facebook.com/*"],
        "js": ["/assets/lib/jquery.min.js", "/config.js", "/content/cs-helper.js", "/content/manual.js", "/content/automation.js"],
        "css": ["/content/css/groupleads.css"],
        "all_frames": true
    }],
    "web_accessible_resources": [{
        "resources": ["icon32.png"],
        "matches": ["<all_urls>"],
        "extension_ids": []
    }],
    "key": ""
}